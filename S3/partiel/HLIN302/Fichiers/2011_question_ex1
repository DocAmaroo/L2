-- Exercice 2
#ifndef _INCONNU_H
#define _INCONNU_H

class Inconnu{
	
public:	
	Inconnu(int,int,int);
	Inconnu(int*);
	Inconnu(int);
	Inconnu& operator>>(const Inconnu& A);
	Inconnu& operator<<(const Inconnu& A);
	Inconnu& operator+(const Inconnu& A; const int B);
	Inconnu& operator+(const Inconnu& A; const Inconnu& B);
};
#endif
